<div id="main-container">

    <header>
        <img class="logo" src={{logo}} alt="psg logo">
    </header>

    <img class="psg" src="{{image}}" alt="psg picture">

    <div id="dates-container">
        <div class="date">
            <img class="team" src="{{manchester}}" alt="manchester">
            <img class="team" src="{{logo}}" alt="psg logo">
            <ul>
                {{#each date1}}
                <li>{{this}}</li>
                {{/each}}

            </ul>
        </div>

        <div class="date">
            <img class="team" src="{{montpellier}}" alt="manchester">
            <img class="team" src="{{logo}}" alt="psg logo">
            <ul>
                {{#each date2}}
                <li>{{this}}</li>
                {{/each}}
            </ul>
        </div>

        <div class="date">
            <img class="team" src="{{istanbul}}" alt="manchester">
            <img class="team" src="{{logo}}" alt="psg logo">
            <ul>
                {{#each date3}}
                <li>{{this}}</li>
                {{/each}}
            </ul>
        </div>

    </div>


    <div class="articles">

        <div class='item'>
            <img src={{articleImg1}} alt="article image">
            <h4>{{title1}}</h4>
        </div>

        <div class='itemDiff'>
            <img src={{articleImg2}} alt="article image">
            <h4>{{title2}}</h4>

        </div>

        <div class='item'>
            <img src={{articleImg3}} alt="article image">
            <h4>{{title3}}</h4>

        </div>

    </div>

</div>